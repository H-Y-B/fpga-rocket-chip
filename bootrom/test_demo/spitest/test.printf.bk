#include <stdint.h>
#include "myprintf.h"
void probReappear(uint8_t bit8,uint32_t bit32){
    printf("8bit printf %d | uartsend ", bit8);
    uart_send(bit8+'0');
    printf("\n\r32bit printf %d | uartsend ", bit32);
    uart_send(bit32+'0');
}
